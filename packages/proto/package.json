{
	"name": "@wha.ts/proto",
	"version": "0.1.1",
	"description": "Protobuf definitions for WhatsApp communication",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": ["dist"],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"development": "./src/index.ts",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"keywords": ["whatsapp", "typescript", "api", "protobuf"],
	"author": "Jo√£o Lucas de Oliveira Lopes (jlucaso1)",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jlucaso1/wha.ts.git"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"proto:gen": "@bufbuild/buf generate",
		"build": "tsup src/index.ts --format=esm --dts"
	},
	"dependencies": {
		"@bufbuild/protobuf": "^2.2.5"
	},
	"devDependencies": {
		"@bufbuild/protoc-gen-es": "^2.2.5",
		"@bufbuild/buf": "^1.52.1"
	}
}
